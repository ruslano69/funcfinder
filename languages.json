{
  "go": {
    "func_pattern": "^\\s*func\\s+(\\([^)]*\\)\\s+)?(\\w+)\\s*\\(",
    "class_pattern": "^\\s*type\\s+(\\w+)\\s+struct\\s*\\{",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "string_chars": ["\""],
    "raw_string_chars": ["`"],
    "escape_char": "\\"
  },
  "c": {
    "func_pattern": "^\\s*[\\w\\s\\*]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*$",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "string_chars": ["\""],
    "raw_string_chars": [],
    "escape_char": "\\"
  },
  "cpp": {
    "func_pattern": "^\\s*[\\w\\s\\*:<>,]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*(const)?\\s*$",
    "class_pattern": "^\\s*(?:class|struct)\\s+(\\w+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "string_chars": ["\""],
    "raw_string_chars": [],
    "escape_char": "\\"
  },
  "cs": {
    "func_pattern": "^\\s*[\\w\\s\\*<>,\\[\\]]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*$",
    "class_pattern": "^\\s*(?:class|interface)\\s+(\\w+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "string_chars": ["\""],
    "raw_string_chars": ["@\""],
    "escape_char": "\\"
  },
  "java": {
    "func_pattern": "^\\s*[\\w\\s\\*<>,\\[\\]]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*(throws\\s+[\\w,\\s]+)?\\s*\\{?\\s*$",
    "class_pattern": "^\\s*(?:class|interface)\\s+(\\w+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "string_chars": ["\""],
    "raw_string_chars": [],
    "escape_char": "\\"
  },
  "d": {
    "func_pattern": "^\\s*[\\w\\s\\*\\[\\]]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*$",
    "class_pattern": "^\\s*(?:class|struct|interface)\\s+(\\w+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "string_chars": ["\""],
    "raw_string_chars": ["`", "r\""],
    "escape_char": "\\"
  },
  "js": {
    "func_pattern": "^\\s*(?:(export\\s+)?(async\\s+)?function\\s*\\*?\\s+(\\w+)\\s*\\(|(const|let|var)\\s+(\\w+)\\s*=\\s*(async\\s+)?\\()",
    "class_pattern": "^\\s*(?:export\\s+)?class\\s+(\\w+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "string_chars": ["\"", "'"],
    "raw_string_chars": ["`"],
    "escape_char": "\\"
  },
  "ts": {
    "func_pattern": "^\\s*(?:(export\\s+)?(async\\s+)?function\\s*\\*?\\s+(\\w+)\\s*[<(]|(const|let|var)\\s+(\\w+)\\s*=\\s*(async\\s+)?[<(])",
    "class_pattern": "^\\s*(?:export\\s+)?class\\s+(\\w+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "string_chars": ["\"", "'"],
    "raw_string_chars": ["`"],
    "escape_char": "\\"
  },
  "py": {
    "func_pattern": "^\\s*(async\\s+)?def\\s+(\\w+)\\s*\\(",
    "class_pattern": "^\\s*class\\s+(\\w+)",
    "line_comment": "#",
    "block_comment_start": "\"\"\"",
    "block_comment_end": "\"\"\"",
    "string_chars": ["\"", "'"],
    "raw_string_chars": ["r\"", "r'"],
    "escape_char": "\\",
    "indent_based": true
  }
}
