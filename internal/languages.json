{
  "go": {
    "name": "Go",
    "extensions": [".go"],
    "func_pattern": "^\\s*func\\s+(\\([^)]*\\)\\s+)?(\\w+)\\s*\\(",
    "class_pattern": "^\\s*type\\s+(\\w+)\\s+struct\\s*\\{",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*(?:import\\s+\"([^\"]+)\"|\"([^\"]+)\"$)",
    "multi_line_import": "import (",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\""],
    "raw_string_chars": ["`"],
    "escape_char": "\\",
    "exclude_words": ["func"]
  },
  "c": {
    "name": "C/C++",
    "extensions": [".c", ".cpp", ".cc", ".h", ".hpp"],
    "func_pattern": "^\\s*[\\w\\s\\*]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*$",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*#\\s*include\\s*[<\"]([^>\"]+)[>\"]",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\"", "'"],
    "raw_string_chars": [],
    "escape_char": "\\",
    "exclude_words": ["if", "else", "while", "for", "switch", "case", "return", "sizeof"]
  },
  "cpp": {
    "name": "C++",
    "extensions": [".cpp", ".cc", ".cxx", ".hpp", ".h"],
    "func_pattern": "^\\s*[\\w\\s\\*:<>,]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*(const)?\\s*$",
    "class_pattern": "^\\s*(?:[\\w\\s]+\\s+)?(?:class|struct)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*#\\s*include\\s*[<\"]([^>\"]+)[>\"]",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\"", "'"],
    "raw_string_chars": [],
    "escape_char": "\\",
    "exclude_words": ["if", "else", "while", "for", "switch", "case", "return", "sizeof"]
  },
  "cs": {
    "name": "C#",
    "extensions": [".cs", ".csx"],
    "func_pattern": "^\\s*[\\w\\s\\*<>,\\[\\]]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*$",
    "class_pattern": "^\\s*(?:[\\w\\s]+\\s+)?(?:class|interface|struct|enum)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*using\\s+([\\w.]+)",
    "decorator_pattern": "^\\s*\\[(\\w+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\""],
    "raw_string_chars": ["@\""],
    "escape_char": "\\",
    "exclude_words": ["if", "else", "while", "foreach", "for", "switch", "case", "return"]
  },
  "java": {
    "name": "Java",
    "extensions": [".java"],
    "func_pattern": "^\\s*[\\w\\s\\*<>,\\[\\]]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*(throws\\s+[\\w,\\s]+)?\\s*\\{?\\s*$",
    "class_pattern": "^\\s*(?:[\\w\\s]+\\s+)?(?:class|interface|enum)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*import\\s+(?:static\\s+)?([\\w.]+)",
    "decorator_pattern": "^\\s*@(\\w+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\""],
    "raw_string_chars": [],
    "escape_char": "\\",
    "exclude_words": ["if", "else", "while", "for", "switch", "case", "return"]
  },
  "d": {
    "name": "D",
    "extensions": [".d"],
    "func_pattern": "^\\s*[\\w\\s\\*\\[\\]]+\\s+(\\w+)\\s*\\([^)]*\\)\\s*$",
    "class_pattern": "^\\s*(?:[\\w\\s]+\\s+)?(?:class|struct|interface|enum)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*import\\s+([\\w.]+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\""],
    "raw_string_chars": ["`", "r\""],
    "escape_char": "\\",
    "exclude_words": ["if", "else", "while", "foreach", "for", "switch", "case", "return"]
  },
  "js": {
    "name": "JavaScript/TypeScript",
    "extensions": [".js", ".jsx", ".mjs", ".ts", ".tsx"],
    "func_pattern": "^\\s*(?:(export\\s+)?(async\\s+)?function\\s*\\*?\\s+(\\w+)\\s*\\(|(const|let|var)\\s+(\\w+)\\s*=\\s*(async\\s+)?\\()",
    "class_pattern": "^\\s*(?:export\\s+)?class\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*(?:import\\s+.*?from\\s+[\"']([^\"']+)[\"']|require\\s*\\(\\s*[\"']([^\"']+)[\"'])",
    "exclude_patterns": ["^\\.", "^\\./"],
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\"", "'"],
    "raw_string_chars": ["`"],
    "escape_char": "\\"
  },
  "ts": {
    "name": "TypeScript",
    "extensions": [".ts", ".tsx"],
    "func_pattern": "^\\s*(?:(export\\s+)?(async\\s+)?function\\s*\\*?\\s+(\\w+)\\s*[<(]|(const|let|var)\\s+(\\w+)\\s*=\\s*(async\\s+)?[<(])",
    "class_pattern": "^\\s*(?:export\\s+)?class\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*(?:import\\s+.*?from\\s+[\"']([^\"']+)[\"']|require\\s*\\(\\s*[\"']([^\"']+)[\"'])",
    "exclude_patterns": ["^\\.", "^\\./"],
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\"", "'"],
    "raw_string_chars": ["`"],
    "escape_char": "\\"
  },
  "py": {
    "name": "Python",
    "extensions": [".py", ".pyw"],
    "func_pattern": "^\\s*(async\\s+)?def\\s+(\\w+)\\s*\\(",
    "class_pattern": "^\\s*class\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*(?:from\\s+(\\S+)\\s+import|import\\s+(\\S+))",
    "decorator_pattern": "^\\s*@(\\w+)",
    "exclude_patterns": ["^\\.", "^\\.\\.", "^__future__"],
    "line_comment": "#",
    "block_comment_start": "\"\"\"",
    "block_comment_end": "\"\"\"",
    "comment_patterns": ["^\\s*#!", "#.*"],
    "string_chars": ["\"", "'"],
    "raw_string_chars": ["r\"", "r'"],
    "escape_char": "\\",
    "indent_based": true
  },
  "rust": {
    "name": "Rust",
    "extensions": [".rs"],
    "func_pattern": "^\\s*(?:pub\\s+)?(?:async\\s+)?fn\\s+(\\w+)\\s*\\(",
    "class_pattern": "^\\s*(?:pub\\s+)?(?:struct|trait|enum|impl)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*use\\s+([^;]+)",
    "exclude_patterns": ["^super::", "^self::"],
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\""],
    "raw_string_chars": ["r#\"", "r#*[^\"]*#*"],
    "escape_char": "\\"
  },
  "swift": {
    "name": "Swift",
    "extensions": [".swift"],
    "func_pattern": "^\\s*(?:public\\s+)?(?:static\\s+)?func\\s+(\\w+)\\s*\\(",
    "class_pattern": "^\\s*(?:public\\s+)?(?:class|struct|enum|protocol)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*import\\s+(\\S+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\""],
    "raw_string_chars": ["\"\"\".*?\"\"\""],
    "escape_char": "\\",
    "exclude_words": ["func", "if", "while", "for", "guard", "switch", "catch", "return", "throw"]
  },
  "kotlin": {
    "name": "Kotlin",
    "extensions": [".kt", ".kts"],
    "func_pattern": "^\\s*(?:(?:public|private|protected|internal)\\s+)?(?:suspend\\s+)?fun\\s+(?:<[^>]+>\\s+)?(\\w+)\\s*\\(",
    "class_pattern": "^\\s*(?:(?:public|private|protected|internal)\\s+)?(?:data\\s+|sealed\\s+|enum\\s+|abstract\\s+)?(?:class|interface|object)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*import\\s+([\\w.]+)",
    "decorator_pattern": "^\\s*@(\\w+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\""],
    "raw_string_chars": ["\"\"\""],
    "escape_char": "\\",
    "exclude_words": ["if", "else", "while", "for", "when", "switch", "return"]
  },
  "php": {
    "name": "PHP",
    "extensions": [".php"],
    "func_pattern": "^\\s*(?:(?:public|private|protected)\\s+)?(?:static\\s+)?function\\s+(\\w+)\\s*\\(",
    "class_pattern": "^\\s*(?:(?:abstract|final)\\s+)?(?:class|interface|trait)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*(?:use\\s+([\\w\\\\]+)|require(?:_once)?\\s*[\"']([^\"']+)[\"']|include(?:_once)?\\s*[\"']([^\"']+)[\"'])",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*", "#.*"],
    "string_chars": ["\"", "'"],
    "raw_string_chars": [],
    "escape_char": "\\",
    "exclude_words": ["if", "else", "while", "foreach", "for", "switch", "case", "return", "echo", "print"]
  },
  "ruby": {
    "name": "Ruby",
    "extensions": [".rb", ".rake"],
    "func_pattern": "^\\s*def\\s+(\\w+[?!]?)\\s*[\\(\\n]?",
    "class_pattern": "^\\s*(?:class|module)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*require(?:_relative)?\\s*[\"']([^\"']+)[\"']",
    "line_comment": "#",
    "block_comment_start": "=begin",
    "block_comment_end": "=end",
    "comment_patterns": ["#.*"],
    "string_chars": ["\"", "'"],
    "raw_string_chars": ["%q{", "%Q{"],
    "escape_char": "\\",
    "exclude_words": ["if", "elsif", "else", "unless", "while", "until", "for", "case", "when", "return", "yield"]
  },
  "scala": {
    "name": "Scala",
    "extensions": [".scala"],
    "func_pattern": "^\\s*(?:(?:private|protected|public)\\s+)?def\\s+(\\w+)\\s*[\\[\\(]",
    "class_pattern": "^\\s*(?:(?:private|protected|public)\\s+)?(?:case\\s+)?(?:class|object|trait)\\s+(\\w+)",
    "call_pattern": "(\\w+)\\s*\\(",
    "import_pattern": "^\\s*import\\s+([\\w.]+)",
    "line_comment": "//",
    "block_comment_start": "/*",
    "block_comment_end": "*/",
    "comment_patterns": ["//.*"],
    "string_chars": ["\""],
    "raw_string_chars": ["\"\"\""],
    "escape_char": "\\",
    "exclude_words": ["if", "else", "while", "for", "match", "case", "return", "yield"]
  }
}
